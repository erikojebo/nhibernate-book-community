<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="NHibernateBookCommunity.Domain"
                   namespace="NHibernateBookCommunity.Domain.Entities">

  <class name="Review">
    <id name="Id">
      <generator class="native" />
    </id>
    <property name="Title" />
